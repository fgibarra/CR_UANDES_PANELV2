            <log id="_log22" message="(L424) sincronizar: request=${header.request}"/>
            <loop doWhile="true" id="_loop3">
                <simple>${header.listaOperaciones.size} &gt; 0</simple>
                <bean id="_bean14" method="getListaOperaciones" ref="generaDatos"/>
