                <loop doWhile="true" id="_loop4">
                    <simple>${header.listaGruposSincronizar.size} &gt; 0</simple>
                    <choice id="_choice3">
                        <when id="_when7">
                            <simple>${header.countThread.getCounter} &gt; 14</simple>
