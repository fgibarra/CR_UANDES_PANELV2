                            <to id="_to25" uri="seda:sincronizaGrupo"/>
                        </otherwise>
                    </choice>
                </loop>
            </loop>
            <loop doWhile="true" id="_loop4">
                <simple>${header.countThread.getCounter} &gt; 0</simple>
                <delay id="_delay2">
                    <constant>1000</constant>
                </delay>
                <log id="_log32" message="(L451) Esperando counterThread valor=${header.countThread.getCounter}"/>
            </loop>
            <log id="_log25" message="(L453) termina"/>
            <to id="_to26" uri="direct:termina"/>
        </route>
